{
  "expo": {
    "name": "Pomodoro",
    "slug": "Pomodoro-timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#50285b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mvmarcus.Pomodorotimer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Pomodoro Timer developed with React Native, DO NOT USE THIS AS PRODUCTION, it's full of comments, for study purpose only",
    "githubUrl": "https://github.com/00markin/Pomodoro"
  }
}
